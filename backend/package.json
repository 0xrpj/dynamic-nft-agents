    "type": "module",
    "main": "src/server.ts",
    "scripts": {
        "start": "pnpm run build && cross-env NODE_ENV=production node dist/server.js",
        "start:eliza": "tsc && node --loader ts-node/esm src/server.ts",
        "build": "tsc --build && tsc-alias",
        "build:eliza": "tsup src/index.ts --format esm --dts",
        "dev": "cross-env NODE_ENV=development nodemon",
        "heroku-postbuild": "pnpm prune --prod && pnpm install --no-frozen-lockfile && pnpm run build"
    },
